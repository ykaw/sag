(echo =dt $DAYTIME
 echo =time `cat /var/db/ntpd.drift`
 echo =df
 df
 echo =end) >> var/rl0100
$SAGHOME/bin/gengraph
(sleep 180
#cp $SAGHOME/var/*.svg /home/kaw/www/htdocs/sag/.
 cp $SAGHOME/var/*.gif /home/kaw/www/htdocs/sag/.) &
